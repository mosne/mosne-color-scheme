{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mosne/color-scheme",
  "version": "0.1.0",
  "title": "Mosne Color Scheme",
  "category": "widgets",
  "description": "Choose your color scheme using the Interactivity API in an a11y friendly way.",
  "parent": [
    "core/navigation"
  ],
  "example": {},
  "supports": {
    "interactivity": true
  },
  "attributes": {
    "classOptions": {
      "type": "string",
      "default": "has-icon has-label"
    },
    "defaultLabel": {
      "type": "string",
      "default": "Theme"
    },
    "autoLabel": {
      "type": "string",
      "default": "OS auto"
    },
    "darkLabel": {
      "type": "string",
      "default": "Dark"
    },
    "lightLabel": {
      "type": "string",
      "default": "Light"
    },
    "baseColor": {
      "type": "string",
      "default": "#fff"
    },
    "contrastColor": {
      "type": "string",
      "default": "#000"
    }
  },
  "textdomain": "mosne-color-scheme",
  "render": "file:./render.php",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScriptModule": "file:./view.js"
}